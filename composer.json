{
    "name": "lukasmu/laravel-rickroll",
    "description": "Rickroll people trying to break into your Laravel site",
    "keywords": [
        "lukasmu",
        "laravel",
        "rickroll",
        "rick",
        "astley"
    ],
    "homepage": "https://github.com/lukasmu/laravel-rickroll",
    "license": "MIT",
    "authors": [
        {
            "name": "Lukas MÃ¼ller",
            "email": "hello@lukasmu.com",
            "homepage": "https://lukasmu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^3.1",
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.7",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.1",
        "phpstan/phpstan": "^2.1",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LukasMu\\Rickroll\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LukasMu\\Rickroll\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "format": "vendor/bin/pint",
        "lint": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LukasMu\\Rickroll\\RickrollServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
