{
    "name": "lukasmu/laravel-rickroll",
    "description": "Rickroll people trying to break into your Laravel site",
    "keywords": [
        "lukasmu",
        "laravel",
        "rickroll",
        "rick",
        "astley"
    ],
    "homepage": "https://github.com/lukasmu/laravel-rickroll",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Lukas MÃ¼ller",
            "email": "hello@lukasmu.com",
            "homepage": "https://lukasmu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "LukasMu\\Rickroll\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LukasMu\\Rickroll\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LukasMu\\Rickroll\\RickrollServiceProvider"
            ]
        }
    }
}
